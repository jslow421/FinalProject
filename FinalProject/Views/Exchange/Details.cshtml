@model FinalProject.Models.Ticket

@{
    ViewBag.Title = "Details";
}

<h2>Details @Model.eventName</h2>

<ul>
    <li>Event: @Model.eventName</li>
    <li>Date: @Model.date</li>
    <li>Location: @Model.location</li>
    <li>Trade Type: @Model.tradeType</li>
    <li>User: @Model.user</li>
</ul>
<form method="GET" id="ticket" name="ticket">
    @Html.ActionLink("Ticket", "Ticket", new { id = @Model.ticketId},new{ @class = "btn btn-primary" })
</form>


